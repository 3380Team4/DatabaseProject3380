@model ThemeParkApplication.Models.Maintenance

@{
    ViewData["Title"] = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Maintenance</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Order Initiated
        </dt>
        <dd>
            @Html.DisplayFor(model => model.WorkStartDate)
        </dd>
        <dt>
            Finish Date
        </dt>
        <dd>
            @Html.DisplayFor(model => model.WorkFinishDate)
        </dd>
        <dt>
            Attraction ID
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Attr.AttractionId)
        </dd>
        <dt>
            Concession ID
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Conc.ConcessionId)
        </dd>
